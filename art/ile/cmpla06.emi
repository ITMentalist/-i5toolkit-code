/**
 * @file cmpla06.emi
 *
 * Example of OMI instruction CMPSPAD
 * Article ID: CMPLA
 */

dcl spcptr a@ auto              ;
dcl spcptr b@ auto              ;
dcl dd ind-hi char(1) auto      ;
dcl dd ind-lo char(1) auto      ;
dcl dd ind-eq char(1) auto      ;
dcl dd ind-uneq char(1) auto    ;

        cmpspad(i) a@, b@ /
            hi(ind-hi),
            lo(ind-lo),
            eq(ind-eq),
            uneq(ind-uneq)      ;
brk     'LOOK'                  ;
        rtx     *               ;
pend                            ;

