/**
 * @file e005.emi
 *
 * Test of matqat-t declared in emi-inc/mi/q.emi
 */

dcl sysptr qq auto init(
        "E001B", type(q, h'02')
)                               ;
dcl spcptr qatr auto            ;
dcl dd len bin(2) auto          ;

        cpynv len, matqat-t-len ;
        modasa qatr, len        ;
        cpybrep qatr->matqat?tmpl, x'00' ;
        cpynv qatr->matqat?bytes-in, len ;

        matqat qatr, qq         ;

brk '99'                        ;
        neg(s) len              ;
        modasa qatr, len        ;
        rtx *                   ;

/include q.emi                  ;
pend                            ;
